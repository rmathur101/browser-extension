{
  "manifest_version": 3,
  "name": "Example",
  "version": "2.0",

  "description": "Messing around with browser extension APIs",

  "permissions": [
    "activeTab",
    "storage"
  ],

  "action": {
    "default_popup": "views/popup.html",
    "default_icon": {
      "16": "icons/get_started16.png",
      "32": "icons/get_started32.png",
      "48": "icons/get_started48.png",
      "128": "icons/get_started128.png"
    }
  }
}